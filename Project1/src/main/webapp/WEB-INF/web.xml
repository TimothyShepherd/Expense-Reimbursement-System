  </servlet-mapping>

  <servlet>
    <servlet-name>DenyRequest</servlet-name>
    <servlet-class>com.revature.Servlets.DenyRequest</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>DenyRequest</servlet-name>
    <url-pattern>/denyrequest</url-pattern>
  </servlet-mapping>

</web-app>
